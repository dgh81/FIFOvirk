{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "input_CVR",
						"type": "DatasetReference"
					},
					"name": "source1"
				},
				{
					"name": "source2"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: true,",
				"     inferDriftedColumnTypes: true,",
				"     limit: 1,",
				"     httpMethod: 'POST',",
				"     body: '{",
				"    \"_source\":[",
				"        \"Vrvirksomhed.cvrNummer\",",
				"        \"Vrvirksomhed.livsforloeb.periode.gyldigTil\", ",
				"        \"Vrvirksomhed.livsforloeb.periode.gyldigFra\",",
				"        \"Vrvirksomhed.reklamebeskyttet\",",
				"        \"Vrvirksomhed.sidstIndlaest\",",
				"        \"Vrvirksomhed.virksomhedMetadata\",",
				"        \"Vrvirksomhed.enhedstype\"",
				"    ],",
				"    \"track_total_hits\": true,",
				"    \"query\":{",
				"        \"bool\":{",
				"            \"must\":[",
				"                {\"nested\":{",
				"                    \"path\":\"Vrvirksomhed.livsforloeb\",",
				"                    \"query\":{",
				"                        \"bool\": {",
				"                            \"must_not\":[{\"exists\":{\"field\":\"Vrvirksomhed.livsforloeb.periode.gyldigTil\"}}],",
				"                            \"filter\": [{",
				"                                \"range\": {",
				"                                    \"Vrvirksomhed.livsforloeb.periode.gyldigFra\": {",
				"                                        \"gte\": \"1900-01-01\"",
				"                                        ,",
				"                                        \"lte\": \"2025-12-31\"",
				"                                    }",
				"                                }",
				"                            }]",
				"                        }",
				"                    }",
				"                }}",
				"                ,",
				"                {",
				"                    \"bool\": {",
				"                        \"filter\": [",
				"                            {",
				"                                \"range\": {",
				"                                    \"Vrvirksomhed.virksomhedMetadata.nyesteNavn.sidstOpdateret\": {",
				"                                        \"gte\": \"2023-09-20T00:00:00.000\"",
				"                                    }",
				"                                }",
				"                            }",
				"                        ]",
				"                    }",
				"                }",
				"            ]",
				"        }",
				"    }",
				"    ,",
				"    \"size\":10",
				"}',",
				"     timeout: 30,",
				"     headers: ['Content-Type' -> 'application/json'],",
				"     paginationRules: ['supportRFC5988' -> 'true'],",
				"     responseFormat: ['type' -> 'json', 'documentForm' -> 'documentPerLine'],",
				"     dateFormats: ['yyyy/MM/dd'],",
				"     timestampFormats: ['yyyy-MM-dd\\'T\\'HH:mm:ss.SSS\\'Z\\''],",
				"     preferredIntegralType: 'integer',",
				"     preferredFractionalType: 'decimal',",
				"     booleanFormat: ['1', '0'],",
				"     partitionBy('hash', 1)) ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source2"
			]
		}
	}
}